<settings version="2">
    <setting id="addons.unknownsources">true</setting>
    <setting id="audiooutput.volumesteps">50</setting>
    <setting id="locale.country">Russia</setting>
    <setting id="locale.keyboardlayouts">English QWERTY|Russian ЙЦУКЕН</setting>
    <setting id="locale.language">resource.language.ru_ru</setting>
    <setting id="locale.timezone">Europe/Moscow</setting>
    <setting id="locale.timezonecountry">Russia</setting>
    <setting id="lookandfeel.enablerssfeeds">true</setting>
    <setting id="pvrmanager.syncchannelgroups">true</setting>
    <setting id="pvrplayback.enableradiords">false</setting>
    <setting id="pvrplayback.fps">1</setting>
    <setting id="pvrplayback.signalquality">true</setting>
    <setting id="pvrplayback.switchtofullscreen">true</setting>
    <setting id="services.airplay">false</setting>
    <setting id="eventlog.enabled">false</setting>
    <setting id="eventlog.enablednotifications" default="true">false</setting>
    <setting id="videoplayer.stretch43">4</setting>
    <setting id="scrapers.moviesdefault" default="true">metadata.themoviedb.org</setting>
    <setting id="scrapers.musicvideosdefault" default="true">metadata.local</setting>
    <setting id="scrapers.tvshowsdefault" default="true">metadata.tvshows.themoviedb.org</setting>
    
    <general>
        <settinglevel>3</settinglevel>
        <eventlog>
            <level>0</level>
            <showhigherlevels>true</showhigherlevels>
        </eventlog>
    </general>

    <audio>
        <mute>false</mute>
        <fvolumelevel>0.500000</fvolumelevel>
    </audio>
</settings>
